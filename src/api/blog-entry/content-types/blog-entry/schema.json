{
  "kind": "collectionType",
  "collectionName": "blog_entries",
  "info": {
    "singularName": "blog-entry",
    "pluralName": "blog-entries",
    "displayName": "blog-entries"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "text",
      "required": true
    },
    "autors": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::autore.autore",
      "inversedBy": "blog_entries"
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "image": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "date": {
      "type": "date",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "blog_card": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::blog-card.blog-card",
      "mappedBy": "blog_entry"
    }
  }
}
